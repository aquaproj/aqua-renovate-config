{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^\\.github/.*\\.ya?ml$"],
      "matchStrings": [
        " +['\"]?aqua_version['\"]? *: +['\"]?(?<currentValue>[^'\" \\n]+)['\"]?"
      ],
      "depNameTemplate": "aquaproj/aqua",
      "datasourceTemplate": "github-releases"
    }
  ]
}
