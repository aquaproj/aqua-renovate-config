{
  extends: [
    'github>suzuki-shunsuke/renovate-config#3.2.1',
    'github>suzuki-shunsuke/renovate-config:nolimit#3.2.1',
    'github>aquaproj/aqua-renovate-config#2.8.1',
    'github>aquaproj/aqua-renovate-config:file#2.8.1(aqua/imports/.*\\.ya?ml)',
  ],
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/README\\.md/',
      ],
      matchStrings: [
        '(version|ref): "?(?<currentValue>.*)"? +# renovate: depName=(?<depName>.*)',
      ],
      datasourceTemplate: 'github-releases',
    },
    {
      customType: 'regex',
      managerFilePatterns: [
        '/README\\.md/',
      ],
      matchStrings: [
        '"github>aquaproj/aqua-renovate-config#(?<currentValue>[^" \\n\\(]+)',
        '"github>aquaproj/aqua-renovate-config:.*#(?<currentValue>[^" \\n\\(]+)',
        '"github>aquaproj/aqua-renovate-config/.*#(?<currentValue>[^" \\n\\(]+)',
      ],
      datasourceTemplate: 'github-releases',
      depNameTemplate: 'aquaproj/aqua-renovate-config',
    },
    {
      customType: 'regex',
      managerFilePatterns: [
        '/README\\.md/',
      ],
      matchStrings: [
        ' +[\'"]?aqua_version[\'"]? *: +[\'"]?(?<currentValue>[^\'" \\n]+)[\'"]?',
      ],
      depNameTemplate: 'aquaproj/aqua',
      datasourceTemplate: 'github-releases',
    },
    {
      customType: 'regex',
      managerFilePatterns: [
        '/README\\.md/',
      ],
      matchStrings: [
        'raw\\.githubusercontent\\.com/aquaproj/aqua-installer/(?<currentValue>.*?)/aqua-installer',
      ],
      datasourceTemplate: 'github-releases',
      depNameTemplate: 'aquaproj/aqua-installer',
    },
  ],
}
