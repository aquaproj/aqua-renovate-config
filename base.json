{
   "regexManagers": [
      {
         "datasourceTemplate": "github-releases",
         "fileMatch": [
            "\\.?aqua\\.ya?ml"
         ],
         "matchStrings": [
            " +(?:version|'version'|\"version\") *: +(?:(?<currentValue>[^'\" \\n]+)|'(?<currentValue>[^'\" \\n]+)'|\"(?<currentValue>[^'\" \\n]+)\") +# renovate: depName=(?<depName>(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)",
            " +(?:name|'name'|\"name\") *: +(?<depName>(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)@(?<currentValue>[^'\" \\n]+)"
         ]
      },
      {
         "datasourceTemplate": "github-releases",
         "fileMatch": [
            "\\.?aqua\\.ya?ml"
         ],
         "matchStrings": [
            " +(?:ref|'ref'|\"ref\") *: +(?:(?<currentValue>[^'\" \\n]+)|'(?<currentValue>[^'\" \\n]+)'|\"(?<currentValue>[^'\" \\n]+)\") +# renovate: depName=(?<depName>(?<packageName>[^'\" .@/\\n]+/[^'\" @/\\n]+)(/[^'\" /@\\n]+)*)"
         ]
      }
   ]
}
